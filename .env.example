# API Keys for FetchIt
# Copy this file to .env and add your actual API keys

# OpenAI API Configuration
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Cohere API Configuration (Alternative to OpenAI)
REACT_APP_COHERE_API_KEY=your_cohere_api_key_here

# Firebase Configuration (for Cloud Storage)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Optional: AWS S3 Configuration (alternative cloud storage)
REACT_APP_AWS_ACCESS_KEY_ID=your_aws_access_key
REACT_APP_AWS_SECRET_ACCESS_KEY=your_aws_secret_key
REACT_APP_AWS_REGION=us-east-1
REACT_APP_AWS_BUCKET_NAME=your_bucket_name

# OAuth Client IDs and Secrets for Platform Connections
# Google Drive & Gmail
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
REACT_APP_GOOGLE_CLIENT_SECRET=your_google_client_secret

# Microsoft OneDrive & Outlook
REACT_APP_MICROSOFT_CLIENT_ID=your_microsoft_client_id
REACT_APP_MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret

# Dropbox
REACT_APP_DROPBOX_CLIENT_ID=your_dropbox_client_id
REACT_APP_DROPBOX_CLIENT_SECRET=your_dropbox_client_secret

# GitHub
REACT_APP_GITHUB_CLIENT_ID=your_github_client_id
REACT_APP_GITHUB_CLIENT_SECRET=your_github_client_secret

# Slack
REACT_APP_SLACK_CLIENT_ID=your_slack_client_id
REACT_APP_SLACK_CLIENT_SECRET=your_slack_client_secret

# Figma
REACT_APP_FIGMA_CLIENT_ID=your_figma_client_id
REACT_APP_FIGMA_CLIENT_SECRET=your_figma_client_secret

# Trello
REACT_APP_TRELLO_CLIENT_ID=your_trello_client_id
REACT_APP_TRELLO_CLIENT_SECRET=your_trello_client_secret

# Notion API Key
REACT_APP_NOTION_API_KEY=your_notion_integration_token

# Vector Database Configuration
# Chroma Vector Database
REACT_APP_CHROMA_API_KEY=your_chroma_api_key

# Alternative Vector Databases
# Pinecone (if using instead of Chroma)
REACT_APP_PINECONE_API_KEY=your_pinecone_api_key
REACT_APP_PINECONE_ENVIRONMENT=your_pinecone_environment
REACT_APP_PINECONE_INDEX=your_pinecone_index_name

# Supabase (optional - for user management, file metadata, analytics)
REACT_APP_SUPABASE_URL=your_supabase_project_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key
